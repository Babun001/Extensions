{
    "manifest_version": 3,
    "name": "Tab Counter",
    "description": "Counts the number of tabs open in Chrome",
    "version": "1.0",
    "permissions": ["tabs"],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
      "16": "icon.jpg",
      "48": "icon.jpg",
      "128": "icon.jpg"
    }
    },
    "icons":{
      "16": "icon.jpg",
    "48": "icon.jpg",
    "128": "icon.jpg"
    }
  }
  